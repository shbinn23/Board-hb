<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.myapp.user.dao.UserDAO">

    <select id="getUserList" resultType="com.myapp.user.model.UserVO">
        SELECT
            USER_ID,
            ROLE,
            EMAIL,
            PASSWORD,
            USERNAME,
            IS_DELETED
        FROM
            TBL_USER
        WHERE USER_ID = #{userId}
    </select>

</mapper>